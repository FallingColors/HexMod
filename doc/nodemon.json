{
    "watch": [
        "doc/src/hexdoc",
        "doc/properties.toml",
        "Common/src/main/resources/assets/hexcasting/lang"
    ],
    "ignore": ["**/generated/**"],
    "ext": "jinja,html,css,js,ts,toml,json,json5,py",
    "exec": "hexdoc serve doc/properties.toml --src _site/src/docs --dst _site/dst/docs --lang en_us --allow-missing --release"
}
