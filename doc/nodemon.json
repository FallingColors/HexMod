{
    "watch": [
        "doc/src",
        "doc/resources",
        "doc/hexdoc.toml",
        "Common/src/main/resources/assets/*/lang"
    ],
    "ignore": ["**/generated/**"],
    "ext": "jinja,html,css,js,ts,toml,json,json5,py",
    "exec": "hexdoc --quiet-lang ru_ru --quiet-lang zh_cn serve"
}
